package array;

public class array {
	public static void main(String[] args) {
	int[] numeros = new int[5];

	// Preenche o vetor com valores.
	// Os índices são usados para acessar e atribuir valores aos elementos do vetor.
	numeros[0] = 10;
	numeros[1] = 20;
	numeros[2] = 30;
	numeros[3] = 40;
	numeros[4] = 50;
	
	System.out.println(numeros[0]);
}
}