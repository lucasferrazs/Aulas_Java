package exercicios;

public class resolucaoexercicio {
	public static void main(String[] args){
		int numero1 = 100;
		int numero2 = 20;
		
		if(numero1 > numero2){
			System.out.println("o numero 1 é o maior");
		}
		else if(numero2 > numero1) {
			System.out.println("o numero 2 é o maior");
		}
	}
}
